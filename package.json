{
  "name": "yupmd-be",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev2": "cross-env NODE_ENV=development nest start --watch",
    "start:dev": "cross-env NODE_ENV=development nest start -b swc  --watch",
    "seeder:dev": "cross-env NODE_ENV=development yarn start --entryFile seeding",
    "start:debug": "nest start --debug --watch",
    "genSqlType": "yarn kysely-codegen --dialect postgres --env-file .env/.env.development --out-file src/types/kysely-codegen.ts",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint:fix": "eslint ./src --fix",
    "check-type": "npx tsc --noEmit",
    "compile-ts": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsc -p tsconfig.json --generateTrace traceDir",
    "check-ts": "NODE_OPTIONS=\"--max-old-space-size=8192\" npx analyze-trace traceDir"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@mikro-orm/cli": "6.4.6",
    "@mikro-orm/core": "6.4.6",
    "@mikro-orm/migrations": "6.4.6",
    "@mikro-orm/nestjs": "^6.1.1",
    "@mikro-orm/postgresql": "6.4.6",
    "@mikro-orm/seeder": "6.4.6",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/apollo": "^13.0.2",
    "@nestjs/common": "^11.0.9",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.9",
    "@nestjs/graphql": "^13.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.9",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.16",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "graphql-type-json": "^0.3.2",
    "http-proxy-middleware": "^3.0.3",
    "kysely": "^0.27.5",
    "kysely-codegen": "^0.17.0",
    "kysely-knex": "^0.2.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.9",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@typescript/analyze-trace": "^0.10.1",
    "eslint": "9.20.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.15.0",
    "prettier": "^3.5.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
